  "private": true,
  "resolutions": {
    "styled-components": "^5"
  },
  "scripts": {
    "adb-clear": "adb shell pm clear com.jnotes",
    "adb-dev-menu": "adb shell input keyevent 82",
    "adb-restart-server": "adb kill-server && sudo adb start-server",
    "android": "react-native run-android --active-arch-only",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "build:android": "npx react-native build-android --mode=release --active-arch-only",
    "clean": "react-native-clean-project",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "ios:dev": "react-native run-ios --scheme 'SchemeDevelopment'",
    "ios:prod": "react-native run-ios --scheme 'SchemeProduction'",
    "ios:staging": "react-native run-ios --scheme 'SchemeStaging'",
    "postinstall": "patch-package && scripts/get-webapp-mem.sh",
    "preandroid": "cp src/assets/webapp/index.html android/app/src/main/assets/webapp/index.html && npx react-native-asset",
    "prestart": "echo 'copying env' && cp .env.development .env",
    "scrcpy": "scrcpy",
    "scrcpy-tcpip": "scrcpy --tcpip",
    "start": "react-native start --reset-cache --experimental-debugger",
    "test": "jest",
    "update-assets": "cp webapp/dist/index.html src/assets/webapp/index.html && cp webapp/dist/index.html android/app/src/main/assets/webapp/index.html && npx react-native-asset"
  },
  "version": "0.0.1"
